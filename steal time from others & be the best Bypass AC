local plr = game.Players.LocalPlayer
local shit = {
    "ManageSword",
    "Announce"
}
local shit1 = {
    "SetSword ",
    "Player bought soda!",
    "Player bought sword!"
}

-- > Bypass
mt.__namecall = newcclosure(function(self,...)
    local meth = getnamecallmethod()
    local args = {...}
    if meth == "FireServer" then
        if table.find(shit, self.Name) then
            if table.find(shit1, args[1]) then
                bd = true
                return wait(9e9)
            end
        end
    end
    return old(self,...)
end)

print("Bypassing AC")
repeat
    wait()
    plr.Character.Humanoid.WalkSpeed = math.random(1,2)
until bd == true
print("Boom bypassed lol trash AC")
plr.Character.Humanoid.WalkSpeed = 14
