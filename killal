-- bypass by Megumu / egg salad

local tbl = {
	["Aokwkkao"] = true,
	["Xeptly"] = true,
	["celestial_reality"] = true,
	["Potonium"] = true,
	["Calastisy"] = true,
	["RoccoRocksU"] = true,
	["reachifyy44444"] = true,
	["macalads"] = true,
	["Stxphen_X"] = true,
	["Duckscy"] = true,
	["Undistanced"] = true
}

function new(pl)
	if tbl[pl.Name] then
		game.Players.LocalPlayer:Kick("ADMIN HERE")    
	end
end

for i,v in pairs(game.Players:GetPlayers()) do
	new(v)
end

game.Players.PlayerAdded:connect(new)

	while wait() do
		if game:IsLoaded() and game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Backpack then
			wait(1)
			break    
		end
	end



-- Grab their module
local mod
for i,v in pairs(getnilinstances()) do
    if v:IsA("ModuleScript") then
        mod = v
        break
    end
end

-- Skeet
for i,v in pairs(getgc(true)) do
    if typeof(v) == "function" and getfenv(v).script == mod and table.find(debug.getconstants(v), "coroutine") then
        hookfunction(v, function(reason)
            warn("game tried to kick for:",reason)
            return wait(1e19)
        end)
    end
end 

wait(3)

game.Players.LocalPlayer:kick("updating \n pls wait few h")
